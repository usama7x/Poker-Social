{
    "explorer.fileNesting.patterns": {
        "*.ts": "$(capture).js, $(capture).d.ts",
        "*.js": "$(capture).js.map, $(capture).min.js, $(capture).d.ts",
        "*.jsx": "$(capture).js",
        "*.tsx": "$(capture).ts",
        "tsconfig.json": "tsconfig.*.json",
        "package.json": "package-lock.json, .npmrc, yarn.lock, .yarnrc, .eslintrc.js, .prettierrc, commitlint.config.js, readme.md, tsconfig.json, turbo.json, .gitignore, .czrc, babel.config.js"
    },
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true,
        "**/node_modules": true,
        "**/.husky": true,
        "**/.expo": true
    },
    "explorerExclude.backup": null,
    "editor.formatOnSave": true,
    "explorer.fileNesting.enabled": true,
}